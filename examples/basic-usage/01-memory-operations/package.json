{
  "name": "unified-mcp-example-memory-operations",
  "version": "1.0.0",
  "description": "Basic memory operations example for Unified-MCP",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "start": "npm run build && node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "demo": "npm run build && node dist/demo.js",
    "scenario:task-tracking": "npm run build && node dist/scenarios/task-tracking.js",
    "scenario:commit-history": "npm run build && node dist/scenarios/commit-history.js",
    "scenario:ai-conversations": "npm run build && node dist/scenarios/ai-conversations.js",
    "cleanup": "npm run build && node dist/cleanup.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@unified-mcp/client-ts": "workspace:*",
    "@unified-mcp/core": "workspace:*",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "ucp",
    "example",
    "memory",
    "basic",
    "tutorial"
  ],
  "author": "UCP Team",
  "license": "MIT"
}